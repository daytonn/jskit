{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",

  "rules": {
    "array-bracket-spacing": [2, "never"],
    "array-callback-return": [2],
    "block-spacing": [2, "always"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, { "properties": "never" }],
    "comma-spacing": [2, { "before": false, "after": true }],
    "comma-dangle": [2, "only-multiline"],
    "comma-style": [2, "last"],
    "complexity": [2, 4],
    "computed-property-spacing": [2, "never"],
    "consistent-return": [0],
    "consistent-this": [0, "_this"],
    "curly": [0],
    "default-case": [2],
    "dot-location": [2, "property"],
    "dot-notation": [2, { "allowKeywords": true }],
    "eol-last": [2],
    "eqeqeq": [2],
    "func-names": [0],
    "func-style": [0],
    "global-require": [2],
    "handle-callback-err": [2],
    "id-blacklist": [2, "data"],
    "indent": [2, 2, { "SwitchCase": 1 }],
    "jsx-quotes": [2, "prefer-single"],
    "key-spacing": [2],
    "keyword-spacing": [2],
    "linebreak-style": [2, "unix"],
    "lines-around-comment": [0],
    "max-depth": [1],
    "max-len": [0],
    "max-nested-callbacks": [0, 2],
    "max-params": [1, 3],
    "max-statements": [0],
    "max-statements-per-line": [0],
    "new-cap": [0],
    "new-parens": [2],
    "newline-after-var": [0],
    "newline-before-return": [0],
    "newline-per-chained-call": [0, { "ignoreChainWithDepth": 2 }],
    "no-alert": [2],
    "no-array-constructor": [2],
    "no-bitwise": [2],
    "no-caller": [2],
    "no-case-declarations": [2],
    "no-catch-shadow": [2],
    "no-cond-assign": [2],
    "no-console": [1],
    "no-constant-condition": [2],
    "no-continue": [2],
    "no-control-regex": [2],
    "no-debugger": [1],
    "no-delete-var": [2],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-else-return": [2],
    "no-empty": [1],
    "no-empty-character-class": [2],
    "no-empty-function": [0],
    "no-eq-null": [2],
    "no-eval": [2],
    "no-ex-assign": [2],
    "no-extend-native": [2],
    "no-extra-bind": [2],
    "no-extra-boolean-cast": [2],
    "no-extra-label": [2],
    "no-extra-parens": [0],
    "no-extra-semi": [2],
    "no-fallthrough": [2],
    "no-floating-decimal": [2],
    "no-func-assign": [2],
    "no-implicit-coercion": [2, { "allow": ["!!"] }],
    "no-implied-eval": [2],
    "no-inline-comments": [0],
    "no-inner-declarations": [2],
    "no-invalid-regexp": [2],
    "no-invalid-this": [2],
    "no-irregular-whitespace": [2],
    "no-iterator": [2],
    "no-label-var": [2],
    "no-labels": [2],
    "no-lone-blocks": [2],
    "no-lonely-if": [2],
    "no-loop-func": [2],
    "no-magic-numbers": [2, { "ignoreArrayIndexes": true, "ignore": [1, 0], "detectObjects": true }],
    "no-mixed-requires": [2],
    "no-mixed-spaces-and-tabs": [2],
    "no-multi-spaces": [2],
    "no-multi-str": [2],
    "no-multiple-empty-lines": [2, { "max": 1, "maxEOF": 0 }],
    "no-native-reassign": [2],
    "no-negated-condition": [2],
    "no-negated-in-lhs": [2],
    "no-nested-ternary": [2],
    "no-new": [2],
    "no-new-func": [2],
    "no-new-object": [2],
    "no-new-require": [2],
    "no-new-wrappers": [2],
    "no-obj-calls": [2],
    "no-octal": [2],
    "no-octal-escape": [2],
    "no-param-reassign": [2],
    "no-path-concat": [2],
    "no-plusplus": [2],
    "no-proto": [2],
    "no-redeclare": [2],
    "no-regex-spaces": [2],
    "no-restricted-syntax": [0],
    "no-return-assign": [0],
    "no-script-url": [2],
    "no-self-assign": [2],
    "no-self-compare": [2],
    "no-sequences": [2],
    "no-shadow": [2],
    "no-spaced-func": [2],
    "no-sparse-arrays": [2],
    "no-ternary": [0],
    "no-throw-literal": [2],
    "no-trailing-spaces": [2],
    "no-undef": [2, { "typeof": true }],
    "no-undef-init": [2],
    "no-undefined": [2],
    "no-underscore-dangle": [0],
    "no-unexpected-multiline": [2],
    "no-unneeded-ternary": [2],
    "no-unreachable": [2],
    "no-unsafe-finally": [2],
    "no-unused-expressions": [0],
    "no-unused-labels": [2],
    "no-unused-vars": [2],
    "no-useless-call": [2],
    "no-useless-concat": [2],
    "no-useless-escape": [2],
    "no-void": [2],
    "no-whitespace-before-property": [2],
    "no-with": [2],
    "object-curly-spacing": [2, "always"],
    "object-property-newline": [0],
    "one-var": [2, "never"],
    "one-var-declaration-per-line": [2, "always"],
    "operator-assignment": [0],
    "operator-linebreak": [2, "none"],
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "double", { "avoidEscape": true }],
    "radix": [2, "always"],
    "require-jsdoc": [0],
    "semi": [2, "never"],
    "semi-spacing": [2],
    "sort-vars": [0],
    "space-before-blocks": [2],
    "space-before-function-paren": [2, "never"],
    "space-in-parens": [2],
    "space-infix-ops": [2],
    "space-unary-ops": [2],
    "spaced-comment": [0],
    "use-isnan": [2],
    "valid-typeof": [2],
    "wrap-iife": [2, "inside"],
    "wrap-regex": [2],
    "yoda": [2, "never"]
  },

  "env": {
    "amd": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "mocha": true,
    "node": true,
    "shared-node-browser": true,
    "worker": true
  },

  "globals": {
    "_": false,
    "chai": false,
    "expect": false,
    "inject": false,
    "sinon": false
  }
 }
